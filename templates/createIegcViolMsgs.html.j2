{% extends "layoutBase.html.j2" %}
{% block title %}Create IEGC Violation Messages{% endblock %}

{% block head %}
{{ super() }}
{% endblock %}

{% block content %}
<h1>Create IEGC Violation Messages</h1>
<div class="row">
    <div class="col-md-4">
        <form method="POST" enctype="multipart/form-data">
            <div class="form-group">
                <input type="file" name="inpFile" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create Data" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

{% if data and ('message' in data) %}
<div>{{data['message']}}</div>
{% endif %}

{% endblock %}